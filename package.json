{
  "name": "@elliottkember/arduino-leds",
  "version": "1.0.0",
  "description": "JavaScript Arduino LED Emulator",
  "main": "dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "build": "rimraf dist && tsc --sourceMap false && tsc -m commonjs --outDir dist/cjs --sourceMap false",
    "prepare": "npm run build",
    "lint": "eslint src/**/*.ts",
    "test": "npm run lint && jest",
    "test:watch": "jest --watch",
    "benchmark:prepare": "ts-node --project tsconfig.benchmark.json benchmark/convert-instructions.ts",
    "benchmark": "ts-node --project tsconfig.benchmark.json benchmark/index.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^12.12.50",
    "@types/prettier": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@wokwi/elements": "^0.16.2",
    "acorn": "^7.3.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^3.0.9",
    "jest": "^26.1.0",
    "lint-staged": "^9.5.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "avr8js": "^0.12.0"
  }
}
